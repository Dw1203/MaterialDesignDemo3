<UserControl x:Class="DwUserControl.LunBoTu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DwUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
		<Canvas Name="canvas_board" ClipToBounds="True"
            Height="150" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="200">

			<Image Name="image1" Opacity="1.0"
                Canvas.Left="0" Canvas.Top="0" Height="150" Stretch="UniformToFill" Width="200" />

			<Image Name="image2" Opacity="1.0"
                Canvas.Left="0" Canvas.Top="0" Height="150" Stretch="UniformToFill" Width="200" />
		</Canvas>


	</Grid>
</UserControl>
